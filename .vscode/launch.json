{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Application",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Plugin",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/plugins/add_printer/exec.py",
            "console": "integratedTerminal",
            "args": [
                "-c",
                "${workspaceFolder}/plugins/add_printer/params.json"
            ],
            "justMyCode": true
        },  
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}